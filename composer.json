    "homepage": "https://github.org/dayrev/smmry-sdk-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Sposito",
            "homepage": "https://github.com/dsposito"
        }
    ],
    "require": {
        "php": "^7.0",
        "php-curl-class/php-curl-class": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {"DayRev\\Smmry\\": "src/"}
